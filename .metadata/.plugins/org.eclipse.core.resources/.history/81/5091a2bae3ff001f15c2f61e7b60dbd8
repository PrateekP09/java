package in.buildingblocks;
 	
class product {
	String ProductId = "A123";
	String ProductName= "abc";
	static String Category = "ex" ;
}
public class test11 {
	public void showdetail(product obj) {
		System.out.println(obj.ProductId);
		System.out.println(obj.ProductName);
		System.out.println(obj.Category);
	}	
	
	public static void main(String[] args) {
		test11 obj = new test11();
		product a= new product();
		obj.showdetail(a);
	}


}

