package in.buildingblocks;

public class test5 {
	static int ata = 200;

	public static void main(String[] args) {
      
		//ClassName.varName;
		System.out.println(test5.ata);
		
		test5 obj = new test5();
		test5 obj2 = new test5();
		System.out.println(obj.ata);
		System.out.println(obj2.ata);
		
		obj.ata = 500;
		System.out.println(obj.ata);
		System.out.println(obj2.ata);
		
		
	}

}
	